{
  "name": "pdf-to-img",
  "version": "2.1.1",
  "author": "Kyle Hensel",
  "description": "ðŸ“ƒðŸ“¸ Converts PDFs to images in nodejs with no native dependencies",
  "main": "dist",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "pdf",
    "pdfjs",
    "pdf-to-img",
    "pdf-to-png",
    "jest"
  ],
  "repository": "https://github.com/k-yle/pdf-to-img",
  "scripts": {
    "build": "tsc && rm -rf dist && mv temp/src dist && rm -rf temp",
    "lint": "eslint --format pretty --ext .ts --ignore-path .gitignore .",
    "test": "jest",
    "trypublish": "npm publish || true"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "canvas": "2.10.2",
    "pdfjs-dist": "3.1.81"
  },
  "devDependencies": {
    "@types/jest-image-snapshot": "6.1.0",
    "@types/pdfjs-dist": "2.10.377",
    "eslint": "8.29.0",
    "eslint-config-kyle": "9.20.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-image-snapshot": "6.1.0",
    "ts-jest": "29.0.3",
    "typescript": "4.9.4"
  },
  "eslintConfig": {
    "extends": "kyle"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./tests/setupTests.ts"
    ],
    "reporters": [
      "default",
      "jest-image-snapshot/src/outdated-snapshot-reporter.js"
    ],
    "collectCoverage": true
  }
}
